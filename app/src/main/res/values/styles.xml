<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="BoardFontStyle">
        <item name="android:fontFamily">@font/free_mono</item>
<!--        <item name="fontFamily">@font/dancing_script</item>-->
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">#002BC1</item>
        <item name="android:textSize">40sp</item>
        <item name="android:textAllCaps">true</item>
        <item name="android:letterSpacing">0.3</item>
    </style>
    <style name="LabelFontStyle">
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">18sp</item>
    </style>
    <style name="AlertDialogCustom" parent="ThemeOverlay.AppCompat.Dialog.Alert">
        <item name="android:layoutDirection">rtl</item>
    </style>
</resources>